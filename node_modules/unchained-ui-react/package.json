{
  "name": "unchained-ui-react",
  "version": "0.9.0",
  "author": "Masood Ahmed <masood.ahmed@galepartners.com>",
  "license": "GALE",
  "description": "The React Library consisting of Unchained Components",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Gale43/GALE-Unchained-UI-React.git"
  },
  "bugs": {
    "url": "https://gale43.atlassian.net/projects/GALEUC5"
  },
  "keywords": [
    "unchained",
    "UV5",
    "react",
    "JSX",
    "ES6",
    "webpack",
    "eslint",
    "stylelint",
    "babel"
  ],
  "engines": {
    "node": ">=6.2.1",
    "npm": ">=3.9.3"
  },
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 11"
  ],
  "pre-commit": [
    "lint"
  ],
  "scripts": {
    "start": "npm run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "dev": "npm run env:dev -- webpack-dev-server --progress",
    "lint:js": "eslint --ext .js --ext .jsx docs src",
    "lint:scss": "stylelint '{docs,src}/**/*.scss' --syntax scss",
    "lint": "npm run lint:js && npm run lint:scss",
    "build:docs:docgen": "react-docgen -x jsx -e index.js --pretty src/components/containers src/components/controls src/components/layout src/components/media src/components/navigation src/components/other src/components/text -o ./docs/docgenInfo.json",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "cpx 'public/**/*' dist",
    "prebuild": "npm run build:clean && npm run build:copy",
    "build": "npm run env:prod -- webpack"
  },
  "main": "src/components/index.js",
  "devDependencies": {
    "@webpack-blocks/dev-server2": "^0.4.0",
    "@webpack-blocks/webpack2": "^0.4.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-handled-props": "^0.2.3",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-standalone": "^6.24.0",
    "classnames": "^2.2.5",
    "compression-webpack-plugin": "^0.4.0",
    "copy-to-clipboard": "^3.0.5",
    "cpx": "^1.5.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "debug": "^2.6.3",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-no-unused-vars-rest": "^1.0.6",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^2.28.0",
    "js-beautify": "^1.6.12",
    "node-sass": "^4.5.2",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.5.8",
    "raw-loader": "^0.5.1",
    "react": "^15.5.4",
    "react-ace": "^4.2.0",
    "react-docgen": "^2.13.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-router": "^4.1.0",
    "react-router-dom": "^4.1.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.3",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "webpack": "^2.3.3",
    "webpack-blocks-happypack": "^0.1.3",
    "webpack-blocks-split-vendor": "^0.1.2",
    "webpack-dev-server": "^2.4.2"
  }
}
